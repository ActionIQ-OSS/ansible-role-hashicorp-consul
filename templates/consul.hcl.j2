{
  "data_dir": "{{ consul_data_dir }}",
  "bind_addr": "{{ consul_bind_addr }}",
  "client_addr": "{{ consul_client_addr }}",
  {% if consul_wan_join_enabled %}
  "start_join_wan": ["{{ consul_wan_join_addr }}"],
  {% endif %}
  {% if consul_start_join_enabled %}
  "start_join": ["{{ consul_lan_join_addr }}"],
  {% endif %}
  {% if consul_retry_join_enabled %}
  "retry_join": ["provider={{ consul_retry_provider }} tag_key={{ consul_retry_tag_key }} tag_value={{ consul_retry_tag_value }}"],
  {% endif %}
  "datacenter": "{{ consul_datacenter }}",
  "log_file": "{{ consul_log_file }}",
  "log_json": "{{ consul_log_json }}",
  "log_level": "{{ consul_log_level }}",
  "log_rotate_duration": "{{ consul_log_rotate_duration }}"
}
